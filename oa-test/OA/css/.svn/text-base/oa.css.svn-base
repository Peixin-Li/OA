/*面包屑*/
div.breadcrumbs{
	margin: 0 auto;
	width: 1300px;
	font-size: 18px;
	padding: 5px;
	padding-left: 10px;
	position: relative;
	margin-bottom: 5px;
}
div.breadcrumbs > a{
	color: inherit;
}
div.breadcrumbs > span{
	cursor: default;
}
div.breadcrumbs > a:first-child{
	font-size: 22px;
}

/*侧边导航栏*/
div.guide-div > a{
	margin: 0px;
	padding: 0px 0px 0px 5px ;
	height: 40px; 
	display: block;
	color: inherit;
	cursor: default;
	position: relative;
	background: white;
	border-bottom: 1px solid #ddd;
	border-top: 1px solid #ddd;
	/*box-shadow: 0px 2px 5px 0px rgba(0,0,0,0.2);*/
}
div.guide-div > a:first-child{
	border-top: none;
}

div.guide-div > a:hover,
div.guide-div > a:focus,
div.guide-div > a.active{
	text-decoration: none;
}
div.guide-div > a > h4{
	margin: 0px;
	width: 100%;
	padding: 10px;
	font-size: 18px;
	height: 100%;
	color: #336666;
	vertical-align: middle;
}

div.guide-div > ul > li > a:hover,
div.guide-div > ul > li > a:focus,
div.guide-div > ul > li > a.active{
	color: #669999;
	background: #f2f2f2;
}
div.guide-div{
	width: 196px;
	border:1px solid #ddd;
}

/*各模块下的ul*/
ul.page-nav > li:hover,
ul.page-nav > li:focus,
ul.page-nav > li.active{
	/*background: #336666;*/
}
ul.page-nav > li > a{
	color: inherit;
	padding:10px 10px 10px 40px;
	/*text-align: center;*/
	font-size: 15px;
}
ul.page-nav > li > a:hover,
ul.page-nav > li > a:focus,
ul.page-nav > li > a.active{
	/*color: white;
	background: #336666;*/
}
ul.page-nav{
	background: #fafafa;
}



/*消息提示框*/
div.hint{
	background: white;
	top: 10%;
	left: 42%;
	min-width: 300px;
	box-shadow: 0px 0px 5px 5px rgba(0,0,0,0.3);
}
h4.hint-title{
	width: 100%;
	color: white;
	margin: 0px;
}
h3.hint-content{
	width: 100%;
	margin: 0px;
	text-align: center;
	font-size: 15px;
}
div.hint-btns{
	width: 100%;
	padding: 5px;
	margin: 0px;
	padding-bottom: 10px;
}

