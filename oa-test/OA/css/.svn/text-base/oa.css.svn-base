/*用户界面的导航栏*/
ul.user-main-ul > li:hover,
ul.user-main-ul > li:focus,
ul.user-main-ul > li.active{
	background: #669999;
}

/*灰色的a*/
a.hover-ddd:hover,
a.hover-ddd:active,
