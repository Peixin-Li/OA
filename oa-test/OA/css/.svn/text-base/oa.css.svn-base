body{
	margin:0px;
	padding:0px;
	background-size: 100% 100%;
	background-repeat: no-repeat;
	background-attachment:fixed;
	font-family: "Microsoft YaHei" ! important;
}

input,button,select,textarea{
	outline:none
} 

div.main{
	width:100%;
	height:100%;
}

.top-btn:hover,
.top-btn:focus,
.top-btn.active{
	color: #669999;
}

/*水平导航栏*/
div.toptoolsnav{
	border-radius: 15px;
	border:2px solid #669999;
}

ul.main-nav > li{
	padding-top: 3px;
	padding-bottom: 3px;
	border-right: none;
}
ul.main-nav > li:hover,
ul.main-nav > li:focus,
ul.main-nav > li.active{
	/*background: #003333;*/
	background: #336666;
	color: white;
}
ul.main-nav > li.active >a,
ul.main-nav > li:hover >a{
	color: white;
}

ul.main-nav > li > a:hover,
ul.main-nav > li > a:focus{
	background: transparent;
}

.nav .open > a,
.nav .open > a:hover,
.nav .open > a:focus,
.nav .open > a:active{
    border-color: white;
}
ul.main-nav > li > a{
	background-color: transparent;
	width: 100%;
	text-align: center;
	font-size: 16px;
	padding: 5px;
	color: #336666;
	cursor: pointer;
	
}

.nav>li>a:hover,
 .nav>li>a:focus{
 	background: #336666;
 	color: white;
 }
