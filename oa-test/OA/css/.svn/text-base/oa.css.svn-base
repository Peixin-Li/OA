/*不带边框的table*/
.table-unbordered>thead>tr>th,
 .table-unbordered>tbody>tr>th,
  .table-unbordered>tfoot>tr>th,
   .table-unbordered>thead>tr>td,
    .table-unbordered>tbody>tr>td,
     .table-unbordered>tfoot>tr>td{
     	border-top: none;
     	border-bottom: 1px solid white;
     }

/*用户界面首页的渐变色*/
@keyframes userIndexDiv
{
	0%{
		background: #336666;
	}
	100%{
		background: #669999;
	}
}
@-moz-keyframes userIndexDiv
{
	0%{
		background: #336666;
	}
	100%{
		background: #669999;
	}
}
@-webkit-keyframes userIndexDiv
{
	0%{
		background: #336666;
	}
	100%{
		background: #669999;
	}
}
div.user-index-div:hover{
	animation: userIndexDiv 0.8s;
	-moz-animation: userIndexDiv 0.8s;
	-webkit-animation: userIndexDiv 0.8s;
	animation-fill-mode: forwards;
	-moz-animation-fill-mode: forwards;
	-webkit-animation-fill-mode: forwards;
}

/*意见反馈框*/
div.opinion-button-div{
	position: absolute;
	bottom:50%;
	left:0px;
	padding: 10px;
	border-radius:5px;
	left:-5px;
	cursor: pointer;
	border: 2px solid #336666;
}

.table>thead>tr>th{
	border-bottom: 1px solid #ddd;
}

/*用户界面的树形结构*/
.tree {
  width:100%;
}
.tree li {
  list-style-type:none;
  margin:0;
  padding:20px 5px 0 5px;
  position:relative
}
.tree li::before, .tree li::after {
  content:'';
  left:-15px;
  position:absolute;
  right:auto
}
.tree li::before {
  border-left:1px solid #ddd;
  bottom:50px;
  height:100%;
  top:0;
  width:1px
}
.tree li::after {
  border-top:1px solid #ddd;
  height:20px;
  top:45px;
  width:20px
}
.tree li span {
  -moz-border-radius:5px;
  -webkit-border-radius:5px;
  border:1px solid #999;
  border-radius:5px;
  display:inline-block;
  padding:10px 16px;
  text-decoration:none;
  cursor: pointer;
}
.tree li.parent_li>span {
  cursor:pointer
}
.tree>ul>li::before, .tree>ul>li::after {
  border:0;
}
.tree li:last-child::before {
  height:45px
}
.tree li.parent_li>span:hover, .tree li.parent_li>span:hover+ul li span {
  background:#eee;
  border:1px solid #94a0b4;
  color:#000;
}
.tree span:hover{
	background:#eee;
  	border:1px solid #94a0b4;
  	color:#000
}
.tree > ul{
	padding: 0px;
}
.tree span.active{
	background: #5CB85C;
	color: white;
}
.tree span{
	text-align: center;
	background:#fbfbfb;
	width:225px;
}

.oatree span{
	text-align: center;
	background:#fbfbfb;
	width:130px;
}

/*用于显示的pre*/
pre.show-pre{
	background: transparent;
