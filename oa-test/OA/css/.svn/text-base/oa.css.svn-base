ul.yiiPager
{
	font-size:15px;
	font-family: "Microsoft YaHei";
	border:0;
	margin:0;
	padding:0;
	line-height:100%;
	display:inline;
}

ul.yiiPager li
{
	display:inline;
}

ul.yiiPager a:link,
ul.yiiPager a:visited
{
	border:solid 1px rgb(200,200,200);
	color:inherit;
	padding:3px 6px;
	text-decoration:none;
}

ul.yiiPager .page a
{
	font-weight:normal;
}

ul.yiiPager a:hover
{
	border:solid 1px #336666;
}

ul.yiiPager .selected a
{
	background:#336666;
	color:#FFFFFF;
	font-weight:bold;
}

ul.yiiPager .hidden a
{
	border:solid 1px #DEDEDE;
	color:#888888;
}

/**
 * Hide first and last buttons by default.
 */
ul.yiiPager .first,
ul.yiiPager .last
{
	display:inline;
}

/*表格的设置*/
.table > thead > tr > th,
 .table > tbody > tr > th,
  .table > tfoot > tr > th,
   .table > thead > tr > td,
    .table > tbody > tr > td,
     .table > tfoot > tr > td {
    vertical-align: middle;
}

table.structure-table > tbody > tr > td{
	border: none;
}

table{
	word-break:break-all;
}

/*新消息提示框*/
div.newMsg-div{
	position: absolute;
	display: none;
	background: white;
	bottom:0px;
	right:0px;
	width: 300px;
	padding: 0px;
	padding-bottom: 50px;
	border: 2px solid #336666;
}

/*进度条*/
li.flow-li:after{
	display: block;/*很重要，不然会有多余的px,且top left等也不能对齐*/
    content: "";
    width: 0;
    height: 0;
    padding: 0;
    margin: 0;
    position: absolute;
    top: 0px;
    z-index: 10;
    left: 100%;
    border: 29px solid transparent;
    border-left: 29px solid #669999;
}
li.flow-li-red:after{
	display: block;/*很重要，不然会有多余的px,且top left等也不能对齐*/
    content: "";
    width: 0;
    height: 0;
    padding: 0;
    margin: 0;
    z-index: 10;
    position: absolute;
    top: 0px;
    left: 100%;
    border: 29px solid transparent;
    border-left: 29px solid #993333;
}

/*个人资料*/
span.info-edit-span:hover,
span.info-edit-span:focus,
span.info-edit-span:active{
	color: #669999;
}

/*会议室和面试*/
div.meeting-div{
	cursor: pointer;
}
div.meeting-div:hover,
div.meeting-div:focus,
div.meeting-div:active{
